{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "options": {
    "env": {}
  },
  "tasks": [
    {
      "label": "Godot Engine",
      "type": "shell",
      "options": {
        "cwd": "${workspaceFolder}/vsk_godot"
      },
      "command": [
        "scons verbose=no warnings=no werror=yes compiledb=yes precision=double dev_build=no optimize=speed_trace use_static_cpp=true target=editor"
      ],
      "problemMatcher": [],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
