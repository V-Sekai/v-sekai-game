  "version": "0.2.0",
  "options": {
    "env": {
      "RUST_LOG": "error",
      "MVSQLITE_DATA_PLANE": "http://192.168.0.39:7000"
    }
  },
  "configurations": [
    {
      "name": "Build the editor",
      "type": "lldb",
      "request": "launch",
      "args": ["--path", "v-sekai-game", "-e"],
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/godot/bin/godot.macos.editor.dev.double.x86_64",
      "preLaunchTask": "Godot Engine"
    }
  ]
}
